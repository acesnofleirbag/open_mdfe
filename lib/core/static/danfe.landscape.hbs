            <p class="text-lg text-static">{{emitente.ie}}</p>
        </div>
        <div class="quantidade-volumes">
            <p class="text-sm label">QUANTIDADE</p>
            <p class="text-lg text-static-right">{{transportador.produtos.quantidade}}</p>
        </div>
        <div class="especie">
            <p class="text-sm label">ESPÉCIE</p>
            <p class="text-lg text-static">{{transportador.produtos.especie}}</p>
        </div>
        <div class="marca">
            <p class="text-sm label">MARCA</p>
            <p class="text-lg text-static">{{transportador.produtos.marca}}</p>
        </div>
        <div class="numeracao">
            <p class="text-sm label">NUMERAÇÃO</p>
            <p class="text-lg text-static">{{transportador.produtos.numeracao}}</p>
        </div>
        <div class="peso-bruto">
            <p class="text-sm label">PESO BRUTO</p>
            <p class="text-lg text-static-right">{{transportador.produtos.peso.bruto}}</p>
        </div>
        <div class="peso-liquido">
            <p class="text-sm label">PESO LÍQUIDO</p>
            <p class="text-lg text-static-right">{{transportador.produtos.peso.liquido}}</p>
        </div>
    </section>

    <h3 id="dados-produtos" class="font-bold">DADOS DOS PRODUTOS/SERVIÇOS</h3>
    <section id="dados-produtos">
        <div class="quadro-dados-produtos-servicos"></div>
    </section>

    <h3 id="calculo-issqn" class="font-bold">CÁLCULO DO ISSQN</h3>
    <section id="calculo-issqn">
        <div class="inscricao-municipal">
            <p class="text-sm label">INSCRIÇÃO MUNICIPAL</p>
            <p class="text-lg text-static">{{issqn.id}}</p>
        </div>
        <div class="valor-total-servicos">
            <p class="text-sm label">VALOR TOTAL DOS SERVIÇOS</p>
            <p class="text-lg text-static">{{issqn.valor}}</p>
        </div>
        <div class="base-calculo-issqn">

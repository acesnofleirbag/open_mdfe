<body class="container">
    <section id="canhoto">
            <div class="descricao">
                <p class="text-sm text-center">Recebemos de {{emitente.nome}} os produtos e/ou serviços constantes na NFe indicada ao lado.</p>
                <p class="text-sm text-center">Emissão: {{nfe.dataHoraEmissao}} Dest/Reme: {{destinatario.nome}} Valor Total:
                    {{nfe.total}} </p>
            </div>

            <div class="data-recebimento">
                <p class="text-sm label">DATA DO RECEBIMENTO</p>
            </div>

            <div class="identificacao-assinatura-recebedor">
                <p class="text-sm label">IDENTIFICAÇÃO E ASSINATURA DO RECEBEDOR</p>
            </div>

        <div class="nfe-numero-serie">
            <p class="text-lg text-center font-bold">NF-e</p>
            <p class="text-lg text-center font-bold" style="margin: 20px;">N° {{nfe.numero}}</p>
            <p class="text-lg text-center font-bold">Série {{nfe.serie}}</p>
        </div>
    </section>

    <div class="separator"></div>

    <section id="dados-NFe">
        <div class="quadro-identificacao-emitente">
            <p class="font-bold" style="font-size: 48px;">{{emitente.nome}}</p>
            <div>
                <p class="text-base">{{emitente.endereco.logradouro}}, {{emitente.endereco.numero}}</p>
                <p class="text-base" style="margin: 20px 0;">{{emitente.endereco.bairro}} - {{emitente.endereco.municipio}} -
                    {{emitente.endereco.uf}}</p>
                <p class="text-base">CEP: {{emitente.endereco.cep}}</p>
            </div>
            <div>
                <p class="text-base" style="margin-bottom: 20px;">Fone: {{emitente.fone}}</p>
                <p class="text-base">Email: {{emitente.email}}</p>
            </div>
        </div>
        <div class="quadro-descricao-danfe">
            <div>
            <p class="text-lg text-center font-bold">DANFE</p>
            <p class="text-sm text-center">Documento Auxiliar da Nota Fiscal Eletrônica</p>
            </div>
            <div style="width: 100%; display: flex; justify-content: space-around; align-items: center;">
                <div style="border: 1px solid black;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    height: 70px;
                    width: 70px;"
                >
                    <p class="text-lg font-bold">{{nfe.type}}</p>
                </div>
                <div>
                    <p class="text-base">0 - ENTRADA</p>
                    <p class="text-base">1 - SAÍDA</p>
                </div>
            </div>
            <div>
                <p class="text-base font-bold">N° {{nfe.numero}}</p>
                <p class="text-base font-bold" style="margin: 10px 0;">SÉRIE {{nfe.serie}}</p>
                <p class="text-base font-bold">Folha {{page.current}}/{{page.total}}</p>
            </div>
        </div>
        <div class="quadro-codigo-de-barras-chave">
            <img class="codigo-barras-chave" src="" alt="" />
        </div>
        <div class="chave-acesso">
            <p class="text-sm label">CHAVE DE ACESSO</p>
            <p class="text-base font-bold text-static">{{nfe.key}}</p>
        </div>
        <div class="quadro-tipo-operacao"></div>
        <div class="quadro-numero-serie-nfe"></div>
        <div class="quadro-codigo-barras-dados">
            <p class="text-base text-center">Consulta de autenticidade no portal nacional da NF-e
                www.nfe.fazenda.gov.br/portal ou no site da Sefaz autorizadora</p>
        </div>
        <div class="codigo-barras-dados"></div>
        <div class="natureza-operacao">
            <p class="text-sm label">NATUREZA DA OPERAÇÃO</p>
            <p class="text-lg text-static">{{nfe.naturezaOperacao}}</p>
        </div>
        <div class="dados-nfe">
            <p class="text-sm label">PROTOCOLO DE AUTORIZAÇÃO DE USO</p>
            <p class="text-lg text-static font-bold">{{nfe.protocolo}}</p>
        </div>
        <div class="inscricao-estadual-emitente">
            <p class="text-sm label">INSCRIÇÃO ESTADUAL</p>
            <p class="text-lg text-static">{{emitente.ie}}</p>
        </div>
        <div class="inscricao-estadual-st-emitente">
            <p class="text-sm label">INSCRIÇÃO ESTADUAL DO SUBSTITUTO TRIBUTÁRIO</p>
            <p class="text-lg text-static">{{emitente.ieSubstituicao}}</p>
        </div>
        <div class="cnpj-emitente">
            <p class="text-sm label">CNPJ</p>
            <p class="text-lg text-static">{{emitente.cnpj}}</p>
        </div>
    </section>

    <h3 id="destinatario" class="font-bold">DESTINATÁRIO/REMETENTE</h3>
    <section id="destinatario">
        <div class="razao-social">
            <p class="text-sm label">NOME/RAZÃO SOCIAL</p>
            <p class="text-lg text-static">{{destinatario.nome}}</p>
        </div>
        <div class="cnpj">
            <p class="text-sm label">CNPJ/CPF</p>
            <p class="text-lg text-static">{{destinatario.documento}}</p>
        </div>
        <div class="data-emissao">
            <p class="text-sm label">DATA DA EMISSÃO</p>
            <p class="text-lg text-static">{{nfe.dataHoraEmissao}}</p>
        </div>
        <div class="endereco">
            <p class="text-sm label">ENDEREÇO</p>
            <p class="text-lg text-static">{{destinatario.endereco.logradouro}}</p>
        </div>
        <div class="bairro">
            <p class="text-sm label">BAIRRO/DISTRITO</p>
            <p class="text-lg text-static">{{destinatario.endereco.bairro}}</p>
        </div>
        <div class="cep">
            <p class="text-sm label">CEP</p>
            <p class="text-lg text-static">{{destinatario.endereco.cep}}</p>
        </div>
        <div class="data-entrada-saida">
            <p class="text-sm label">DATA DA SAÍDA</p>
            <p class="text-lg text-static">{{nfe.dataEntradaSaida}}</p>
        </div>
        <div class="municipio">
            <p class="text-sm label">MUNICÍPIO</p>
            <p class="text-lg text-static">{{destinatario.endereco.municipio}}</p>
        </div>
        <div class="fone-fax">
            <p class="text-sm label">TELEFONE/FAX</p>
            <p class="text-lg text-static">{{destinatario.fone}}</p>
        </div>
        <div class="uf">
            <p class="text-sm label">UF</p>
            <p class="text-lg text-static">{{destinatario.endereco.uf}}</p>
        </div>
        <div class="inscricao-estadual">
            <p class="text-sm label">INSCRIÇÃO ESTADUAL</p>
            <p class="text-lg text-static">{{destinatario.ie}}</p>
        </div>
        <div class="hora-entrada-saida">
            <p class="text-sm label">HORA DA SAÍDA</p>
            <p class="text-lg text-static">{{nfe.horaEntradaSaida}}</p>
        </div>
    </section>

    <h3 id="duplicatas" class="font-bold">DUPLICATAS</h3>
    <section id="duplicatas">
        <div class="fatura">
            <p class="text-sm label">FATURA</p>
            {{#if duplicatas}}
            <p class="text-lg text-static">{{#each duplicatas}}Número: {{number}}, Vencimento: {{date}},
                Valor: {{value}};{{/each}}</p>
            {{/if}}
        </div>
    </section>
    
    <h3 id="calculo-imposto" class="font-bold">CÁLCULO DO IMPOSTO</h3>
    <section id="calculo-imposto">
        <div class="base-calculo-icms">
            <p class="text-sm label">BASE DE CÁLCULO DO ICMS</p>
            <p class="text-lg text-static-right">{{imposto.baseCalculo}}</p>
        </div>
        <div class="valor-icms">
            <p class="text-sm label">VALOR DO ICMS</p>
            <p class="text-lg text-static-right">{{imposto.valor}}</p>
        </div>
        <div class="base-calculo-icms-st">
            <p class="text-sm label">BASE DE CÁLCULO DO ICMS SUBSTITUIÇÃO</p>
            <p class="text-lg text-static-right">{{imposto.baseCalculoSubstituicao}}</p>
        </div>
        <div class="valor-icms-st">
            <p class="text-sm label">VALOR DO ICMS SUBSTITUIÇÃO</p>
            <p class="text-lg text-static-right">{{imposto.valorSubstituicao}}</p>
        </div>
        <div class="valor-total-dos-produtos">
            <p class="text-sm label">VALOR TOTAL DOS PRODUTOS</p>
            <p class="text-lg text-static-right">{{imposto.total}}</p>
        </div>
        <div class="valor-frete">
            <p class="text-sm label">VALOR DO FRETE</p>
            <p class="text-lg text-static-right">{{imposto.frete}}</p>
        </div>
        <div class="valor-seguro">
            <p class="text-sm label">VALOR DO SEGURO</p>
            <p class="text-lg text-static-right">{{imposto.seguro}}</p>
        </div>
        <div class="desconto">
            <p class="text-sm label">DESCONTO</p>
            <p class="text-lg text-static-right">{{imposto.desconto}}</p>
        </div>
        <div class="outras-despesas-acessorias">
            <p class="text-sm label">OUTRAS DESPESAS ACESSÓRIAS</p>
            <p class="text-lg text-static-right">{{imposto.outro}}</p>
        </div>
        <div class="valor-ipi">
            <p class="text-sm label">VALOR DO IPI</p>
            <p class="text-lg text-static-right">{{imposto.ipi}}</p>
        </div>
        <div class="valor-total-nota">
            <p class="text-sm label">VALOR TOTAL DA NOTA</p>
            <p class="text-lg text-static-right">{{nfe.total}}</p>
        </div>
    </section>

    <h3 id="transportador" class="font-bold">TRANSPORTADOR/VOLUMES TRANSPORTADOS</h3>
    <section id="transportador">
        <div class="razao-social">
            <p class="text-sm label">NOME/RAZÃO SOCIAL</p>
            <p class="text-lg text-static">{{emitente.nome}}</p>
